#include <iostream>
#include <iomanip>  
using namespace std;

int main()
{
	const int s=216;
	double x[s];
	for(int i=0;i<s;i++)
	{
		cin>>x[i];
	}
	cout<<x[0]<<" ";
	for(int k=1;k<s-1;k++)
	{
		double z=(x[k-1]+x[k]+x[k+1])/3;
		cout<<setprecision(12)<<z<<" ";
	}
	cout<<x[s-1];
	cout<<endl;
	system("pause");
	return 0;
}
